    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "build:worker": "opennextjs-cloudflare build",
    "dev:worker": "wrangler dev --port 8771",
    "watch:build": "npx watch 'npm run build:worker' -- src",
    "build:cron:test": "./run_cron_test.sh",
    "trigger:cron": "./scripts/trigger-cron-jobs.sh",
    "playground": "tsx scripts/standalone-playground.ts",
    "analyze": "ANALYZE=true next build",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:pure": "vitest run tests/unit/cache-utils.test.ts tests/unit/transformers.test.ts tests/unit/config.test.ts",
    "test:unit": "vitest run tests/unit",
    "model-test": "tsx scripts/model-comparison-test.ts",
    "model-score": "tsx scripts/model-scoring.ts",
    "test:exec-summary": "tsx scripts/test-executive-summary.ts"
